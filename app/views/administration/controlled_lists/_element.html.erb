<%= fields_for 'administration_controlled_list[elements][]', element, index: nil do |fields| %>
    <div class="<%= "empty-entry" unless element.persisted? %>">
      <%= fields.hidden_field :id %>
      <div class="form-group">
        <label class="control-label col-sm-2">Entry</label>
        <div class="col-sm-4">
          <%= fields.text_field :name, class: 'form-control' %>
        </div>
      </div>
    </div>
<% end %>