<h4>Bibliografisk data</h4>
<table class="table table-striped"><tbody>
<tr>
  <td>
    <strong>ISBN</strong>
  </td>
  <td>
    <span><%= @instance.isbn13 %></span>
  </td>
</tr>
<tr>
<tr>
  <td>
    <strong>Copyright Date</strong>
  </td>
  <td>
    <span><%= @instance.copyright_date %></span>
  </td>
</tr>
<tr>
  <td>
    <strong>Date published</strong>
  </td>
  <td>
    <span><%= @instance.published_date %></span>
  </td>
</tr>
<tr>
  <td>
    <strong>Mode of issuance </strong>
  </td>
  <td>
    <span><%= @instance.mode_of_issuance %></span>
  </td>
</tr>
<tr>
  <td>
    <strong>Extent</strong>
  </td>
  <td>
    <span><%= @instance.extent %></span>
  </td>
</tr>
<tr>
  <td>
    <strong>Language </strong>
  </td>
  <td>
    <span><%= @instance.language_values %></span>
  </td>
</tr>
<tr>
  <td>
    <strong>Note </strong>
  </td>
  <td>
    <% @instance.note.each do |n| %>
    <span>
      <%= n %>
    </span>
    <% end %>
  </td>
</tr>

</tbody></table>
