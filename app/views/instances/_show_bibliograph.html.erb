<a id="bibliotgraphic"></a><h4>Bibliografisk data</h4>
<table class="table table-striped"><tbody>
<tr>
  <td>
    <strong>ISBN</strong>
  </td>
  <td>
    <span><%= @instance.isbn13 %></span>
  </td>
</tr>
<tr>
<tr>
  <td>
    <strong><%=  t('.copyright_date') %></strong>
  </td>
  <td>
    <span><%= @instance.copyright_date %></span>
  </td>
</tr>
<tr>
  <td>
    <strong><%= t('.publisher_name') %></strong>
  </td>
  <td>
    <span><%= @instance.publisher_name %></span>
  </td>
</tr>
<tr>
  <td>
    <strong><%= t('.published_place') %></strong>
  </td>
  <td>
    <span><%= @instance.published_place %></span>
  </td>
</tr>
<tr>
  <td>
    <strong><%= t('.published_date') %></strong>
  </td>
  <td>
    <span><%= @instance.published_date %></span>
  </td>
</tr>
<tr>
  <td>
    <strong><%= t('.mode_of_issuance') %></strong>
  </td>
  <td>
    <span><%= @instance.mode_of_issuance %></span>
  </td>
</tr>
<tr>
  <td>
    <strong><%= t('.extent') %></strong>
  </td>
  <td>
    <span><%= @instance.extent %></span>
  </td>
</tr>
<tr>
  <td>
    <strong><%= t('.language') %></strong>
  </td>
  <td>
    <%# Show language codes as collection of labels, rather than array of codes %>
    <span><%= @instance.language_values.collect { |lang| get_entry_label('Languages', lang) }.join(', ') %></span>
  </td>
</tr>
<tr>
  <td>
    <strong>Note </strong>
  </td>
  <td>
    <% @instance.note.each do |n| %>
    <span>
      <%= n %>
    </span>
    <% end %>
  </td>
</tr>

</tbody></table>
